# Database (MySQL)
DATABASE_URL="mysql://username:password@localhost:3306/leadmind"

# Admin Authentication
# 개발 환경: ADMIN_PASSWORD를 사용
ADMIN_PASSWORD="your-secure-password"

# 프로덕션 환경: ADMIN_PASSWORD_HASH를 사용 (권장)
# npm run hash-password <your-password> 명령으로 해시 생성
# ADMIN_PASSWORD_HASH="$2b$12$..."

# JWT Secret (프로덕션에서 반드시 변경)
JWT_SECRET="your-secure-jwt-secret-min-32-characters"

# Google Sheets Admin Key (시트 헤더 업데이트용)
ADMIN_SHEET_KEY="your-secure-admin-sheet-key"
